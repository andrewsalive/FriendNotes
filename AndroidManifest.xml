<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.test.friendnotes" >
    
    <uses-sdk android:minSdkVersion="8"
       	 	android:targetSdkVersion="17" />

    <application android:icon="@drawable/icon" >
        <activity
            android:name="com.android.test.friendnotes.FriendsList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.test.friendnotes.FriendsNotes" />
        <activity
            android:name="com.android.test.friendnotes.FriendEditProfile"
            android:label="@string/edit_user" >
        </activity>
        <activity
            android:name="com.android.test.friendnotes.NoteEdit"
            android:label="@string/edit_note" >
        </activity>
    </application>

</manifest>